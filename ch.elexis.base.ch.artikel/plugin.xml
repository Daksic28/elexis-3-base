            ImporterClass="ch.elexis.base.ch.artikel.data.MedikamentImporter"
            name="Medikament">
      </Artikel>
      <Artikel
            CodeDetailDisplay="ch.elexis.base.ch.artikel.views.MiGelDetailDisplay"
            CodeSelectorFactory="ch.elexis.base.ch.artikel.views.MiGelSelector"
            ElementFactory="ch.elexis.base.ch.artikel.data.ArtikelFactory"
            ImporterClass="ch.elexis.base.ch.artikel.data.MiGelImporter"
            name="MiGeL">
      </Artikel>
   </extension>

